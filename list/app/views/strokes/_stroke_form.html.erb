<%= form_for Stroke.new do |f| %>
	<td><%= image_tag "default.png", :size => "70x50" %></td>
	<td><%= f.text_field :content,
						 :class => "form-control ui-autocomplete-input",
						 :placeholder => "content",
						 :id => "content",
						 :autocomplete => "off"%>
	<td><%= f.text_field :tags,
						 :class => "form-control",
						 :placeholder => "tag"%>
	<td><%= f.submit :class => "btn btn-default"%>
<% end %>